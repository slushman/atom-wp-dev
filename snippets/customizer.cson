".text.html.php":
	"WPCustomizerPanel":
		"prefix": "wpcp"
		"body": """// ${1:panel_name} Panel
			$wp_customize->add_panel( '${2:panel_id}',
				array(
					'capability'  		=> 'edgepait_theme_options',
					'description'  		=> esc_html__( '${3:description}', '${4:text-domain}' ),
					'priority'  		=> 10,
					'theme_supports'  	=> '',
					'title'  			=> esc_html__( '${1:panel_name}', '${4:text-domain}' ),
				)
			);"""
	"WPCustomizerSection":
		"prefix": "wpcs"
		"body": """// ${1:section_name} Section
			$wp_customize->add_section( '${2:section_id}',
				array(
					'active_callback' 	=> '${3:callback_function}',
					'capability'  		=> 'edit_theme_options',
					'description'  		=> esc_html__( '${4:description}', '${5:text-domain}' ),
					'panel' 			=> '${1:panel_id}',
					'priority'  		=> 10,
					'theme_supports'  	=> '',
					'title'  			=> esc_html__( '${1:section_name}', '${5:text-domain}' ),
				)
			);"""
	"WPCustomizerText":
		"prefix": "wpct"
		"body": """// ${1:field_name} Field
